<div class="container-fluid">
  <div class="row">
    <div class="col-md-offset-4 col-md-4">
      <h1 class="bg-primary text-center p25">
        Bootstrapped!
      </h1>
    </div>
  </div>

  Addresses:
  <br/>

  <table border="1">
  <thead>
    <tr>
      <th>Coordinates</th>
      <th>Address</th>
    </tr>
  </thead>
  <tbody>

    <% addresses.map do |address| %>
      <tr>
        <td><%= address.lat %>, <%= address.lng %></td>
        <td><%= address.full_address %></td>
      </tr>
    <% end %>

  </tbody>
  </table>
</div>
